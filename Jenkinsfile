pipeline {
    agent any
    stages{
        stage("Clone"){
            steps {
                println "Here we clone code"
            }

        }
        stage("Build"){
             steps {
                println "Here we build code"
            }

        }
        stage("Upload artifacts"){

              steps {
                println "Here we upload artifacts"
            }

        }
        stage("Deploy"){
                steps {
                println "Here we deploy artifacts"
            }
        }
    }
}